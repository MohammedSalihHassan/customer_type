<?xml version="1.0" encoding="utf-8"?>
<odoo>   
    <!-- Sequences for customer type -->
    <record id="seq_customer_type" model="ir.sequence">
        <field name="name">Customer Type</field>
        <field name="code">res.partner</field>
        <field name="prefix">CT/%(year)s%(month)s%(day)s%(h24)s%(min)s%(sec)s/</field>
        <field name="padding">4</field>
        <field name="number_next">2</field>
        <field name="number_increment">1</field>
        
        <field name="company_id" eval="False"/>
    </record>
</odoo>